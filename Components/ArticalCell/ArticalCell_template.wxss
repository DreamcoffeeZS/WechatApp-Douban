
.arcicalCell_container{
	display: flex;
	flex-direction: column;
	margin-left: 30rpx;
	margin-right: 30rpx;
	margin-bottom: 50rpx;
	background-color: #fff;
	border:1rpx solid #eeeeee;
}

.aricalCell_title{
	color: #333333;
	font-size: 17px;
	font-weight: 600;
	margin-top: 20px;
	margin-left: 30rpx;
	margin-right: 30rpx;
}

.aricalCell_info{
	display: flex;
	flex-direction: row;
	align-items: center;
	margin-top: 20rpx;
	margin-left: 30rpx;
	margin-bottom: 20rpx;
	font-size: 12px;
	color: #999999;
}


.aricalCell_create{
	margin-right: 20px;
}

.articalCell_icon{
	width:30rpx;
	height: 30rpx;
	margin-right: 5px;
}


.articalCell_brief{
	margin-left: 30rpx;
	margin-right: 30rpx;
	margin-bottom: 20rpx;
	font-size: 14px;
	color: #999999;
}

.articalCell_continue{
	display: flex;
	flex-direction: row;
	justify-content: flex-end;
	margin-left: 30rpx;
	margin-right: 30rpx;
	height:60rpx;
	margin-bottom: 30rpx;
}

.articalCell_continueText{
	width: 150rpx;
	height: 60rpx;
	background-color: #f42c28;
	color: white;
	border-radius:8rpx;
	font-size: 14px;
	text-align: center;
    line-height: 60rpx;
}




